<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" thread-count="3" parallel="tests" verbose="3">

<test name="Google Test">
<parameter name="url" value="https://www.google.com"/>
<parameter name="browser" value="chrome"/>
	<classes>
		<class name="TestNGTest.GoogleDemo"/>
	</classes>
</test>

<test name="Facebook">
<parameter name="url" value="https://www.Facebook.com"></parameter>
<parameter name="browser" value="firefox"/>
	<classes>
		<class name="TestNGTest.FacebookDemo"/>
	</classes>
</test>


<test name="Gmail">
<parameter name="url" value="https://www.udemy.com/"/>
<parameter name="browser" value="edge"/>
	<classes>
		<class name="TestNGTest.GmailDemo"/>
	</classes>
</test>

</suite>